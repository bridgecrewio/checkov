{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "my-nginx",
    "namespace": "nginx",
    "labels": {
      "app": "nginx"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "nginx",
        "image": "nginx:1.14.2",
        "ports": [
          {
            "name": "http",
            "containerPort": 80
          }
        ]
      }
    ]
  }
}
