{
  "apiVersion": "extensions/v1beta1",
  "kind": "Deployment",
  "metadata": {
    "creationTimestamp": null,
    "labels": {
      "app": "helm",
      "name": "tiller"
    },
    "name": "tiller-deploy",
    "namespace": "kube-system"
  },
  "spec": {
    "strategy": {},
    "template": {
      "metadata": {
        "creationTimestamp": null,
        "labels": {
          "app": "helm",
          "name": "tiller"
        }
      },
      "spec": {
        "containers": [
          {
            "args": ["--listen=localhost:12345"],
            "env": [
              {
                "name": "TILLER_NAMESPACE",
                "value": "kube-system"
              },
              {
                "name": "TILLER_HISTORY_MAX",
                "value": "0"
              }
            ],
            "image": "gcr.io/kubernetes-helm/tiller:v2.8.0",
            "imagePullPolicy": "IfNotPresent",
            "livenessProbe": {
              "httpGet": {
                "path": "/liveness",
                "port": 44135
              },
              "initialDelaySeconds": 1,
              "timeoutSeconds": 1
            },
            "name": "tiller",
            "ports": [
              {
                "containerPort": 44134,
                "name": "tiller"
              }
            ],
            "readinessProbe": {
              "httpGet": {
                "path": "/readiness",
                "port": 44135
              },
              "initialDelaySeconds": 1,
              "timeoutSeconds": 1
            },
            "resources": {}
          }
        ]
      }
    }
  },
  "status": {}
}