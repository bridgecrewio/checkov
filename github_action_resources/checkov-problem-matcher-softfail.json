{
  "problemMatcher": [
    {
      "owner": "checkov",
      "pattern": [
        {
          "regexp": "^Check: (\\w+: .*)$",
          "message": 1
        },
        {
          "regexp": "^\\WFAILED.*$"
        },
        {
          "regexp": "^\\WFile: \/(.+):(\\d+)-(\\d+)$",
          "file": 1,
          "line": 2
        }
      ],
      "severity": "error"
    }
  ]
}
